<div class="tracks">
  <div class="nav">
    <h3>Title</h3>
    <h3>Listen</h3>
  </div>
  {{#each tracks}}
    <div class="track">
      <div class="title-song">
        <p>{{name}}</p>
      </div>
      <div class="player">
        <audio controls="controls" src="{{preview_url}}">
          <source src="audiofile.mp3" type="audio/mpeg" />
          <source src="audiofile.ogg" type="audio/ogg" />
        </audio>
      </div>
    </div>
  {{/each}}
</div>